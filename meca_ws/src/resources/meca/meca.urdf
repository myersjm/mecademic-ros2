    <limit effort="10.0" velocity="5.23599" lower="-2.00713" upper="2.00713" />
  </joint>

  <!-- Joint 6 -->
  <link name="meca_axis_6_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="meshes/meca_500_r3_j6.dae" />
